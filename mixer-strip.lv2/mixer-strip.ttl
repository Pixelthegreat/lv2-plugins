@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .

<http://fanfavoritessofar.com/mixer-strip>
	a lv2:Plugin ,
		lv2:AmplifierPlugin ;
	lv2:project <http://fanfavoritessofar.com/mixer-strip> ;
	doap:name "Mixer Strip" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	ui:ui <http://fanfavoritessofar.com/mixer-strip#ui-x11> ;
	lv2:port [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 0 ;
		lv2:symbol "inputGain" ;
		lv2:name "Input Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -90.0 ;
		lv2:maximum 24.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+5" ;
			rdf:value 5.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-5" ;
			rdf:value -5.0
		] , [
			rdfs:label "-10" ;
			rdf:value -10.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 1 ;
		lv2:symbol "inputPan" ;
		lv2:name "Input Pan" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
		lv2:maximum 1.0 ;
		lv2:scalePoint [
			rdfs:label "Left" ;
			rdf:value -1.0 ;
		] , [
			rdfs:label "Center" ;
			rdf:value 0.0 ;
		] , [
			rdfs:label "Right" ;
			rdf:value 1.0 ;
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "lowEQ" ;
		lv2:name "Low EQ" ;
		lv2:default 0.0 ;
		lv2:minimum -12.0 ;
		lv2:maximum 12.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+12" ;
			rdf:value 12.0
		] , [
			rdfs:label "+6" ;
			rdf:value 6.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-6" ;
			rdf:value -6.0
		] , [
			rdfs:label "-12" ;
			rdf:value -12.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "midEQ" ;
		lv2:name "Mid EQ" ;
		lv2:default 0.0 ;
		lv2:minimum -12.0 ;
		lv2:maximum 12.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+12" ;
			rdf:value 12.0
		] , [
			rdfs:label "+6" ;
			rdf:value 6.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-6" ;
			rdf:value -6.0
		] , [
			rdfs:label "-12" ;
			rdf:value -12.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "highEQ" ;
		lv2:name "High EQ" ;
		lv2:default 0.0 ;
		lv2:minimum -12.0 ;
		lv2:maximum 12.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+12" ;
			rdf:value 12.0
		] , [
			rdfs:label "+6" ;
			rdf:value 6.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-6" ;
			rdf:value -6.0
		] , [
			rdfs:label "-12" ;
			rdf:value -12.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "outputGain" ;
		lv2:name "Output Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -90.0 ;
		lv2:maximum 24.0 ;
		units:unit units:db ;
		lv2:scalePoint [
			rdfs:label "+5" ;
			rdf:value 5.0
		] , [
			rdfs:label "0" ;
			rdf:value 0.0
		] , [
			rdfs:label "-5" ;
			rdf:value -5.0
		] , [
			rdfs:label "-10" ;
			rdf:value -10.0
		]
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "outputPan" ;
		lv2:name "Output Pan" ;
		lv2:default 0.0 ;
		lv2:minimum -1.0 ;
		lv2:maximum 1.0 ;
		lv2:scalePoint [
			rdfs:label "Left" ;
			rdf:value -1.0 ;
		] , [
			rdfs:label "Center" ;
			rdf:value 0.0 ;
		] , [
			rdfs:label "Right" ;
			rdf:value 1.0 ;
		]
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 7 ;
		lv2:symbol "leftInput" ;
		lv2:name "Left Input" ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 8 ;
		lv2:symbol "rightInput" ;
		lv2:name "Right Input" ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 9 ;
		lv2:symbol "leftOutput" ;
		lv2:name "Left Output" ;
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 10 ;
		lv2:symbol "rightOutput" ;
		lv2:name "Right Output" ;
	] .
