@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .

<http://fanfavoritessofar.com/delay>
	a lv2:Plugin ,
		lv2:AmplifierPlugin ;
	lv2:project <http://fanfavoritessofar.com> ;
	doap:name "Delay" ;
	lv2:optionalFeature lv2:hardRTCapable ;
	ui:ui <http://fanfavoritessofar.com/delay#ui-x11> ;
	lv2:port [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "leftInput" ;
		lv2:name "Left Input"
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 1 ;
		lv2:symbol "rightInput" ;
		lv2:name "Right Input"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 2 ;
		lv2:symbol "leftOutput" ;
		lv2:name "Left Output"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 3 ;
		lv2:symbol "rightOutput" ;
		lv2:name "Right Output"
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "time" ;
		lv2:name "Time" ;
		lv2:default 50.0 ;
		lv2:minimum 0.0 ;
		lv2:maximum 200.0 ;
		units:unit units:ms ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "feedback" ;
		lv2:name "Feedback" ;
		lv2:default 0.2 ;
		lv2:minimum 0.1 ;
		lv2:maximum 0.9 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "inputGain" ;
		lv2:name "Input Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -12.0 ;
		lv2:maximum 12.0 ;
		units:unit units:db ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "outputGain" ;
		lv2:name "Output Gain" ;
		lv2:default 0.0 ;
		lv2:minimum -12.0 ;
		lv2:maximum 12.0 ;
		units:unit units:db ;
	] .
